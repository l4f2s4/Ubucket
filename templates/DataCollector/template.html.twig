{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon"><img src="data:image/png;base64," alt="Git"/></span>
        <span class="sf-toolbar-status">{{ collector.count('nocache') }} / {{ collector.count('cache') }}</span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
        </div>
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}
{% endblock %}

{% block menu %}
    <span class="label label-status-warning">
        <span class="icon"><img src="data:image/png;base64," alt=""/></span>
        <strong>Git calls</strong>
        <span class="count">
            <span>{{ collector.count('nocache') }} / {{ collector.count('cache') }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Git calls (Not cached)</h2>
    <table>
        <tr>
            <th>Count</th>
            <th>Type</th>
            <th>Sha</th>
        </tr>

{% for item in collector.calls('nocache') %}
    <tr>
        <td>{{ item.count }}</td>
        <td>{{ item.type }}</td>
        <td>{{ item.sha }}</td>
    </tr>
{% endfor %}

    </table>


    <h2>Git calls (Cached)</h2>
    <table>
        <tr>
            <th>Count</th>
            <th>Type</th>
            <th>Sha</th>
        </tr>

{% for item in collector.calls('cache') %}
    <tr>
        <td>{{ item.count }}</td>
        <td>{{ item.type }}</td>
        <td>{{ item.sha }}</td>
    </tr>
{% endfor %}

    </table>
{% endblock %}
